{
  "version": 2,
  "builds": [
    {
      "src": "app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "app/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app/$1"
    }
  ],
  "buildCommand": "cd app && npm run build",
  "installCommand": "cd app && npm install",
  "outputDirectory": "app/build",
  "framework": "create-react-app"
}